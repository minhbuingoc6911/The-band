		jQuery("#txtErrorLoginEmail2_Regis").html("Vui lòng nhập Xác nhận Email");
		jQuery("#txtCEmail_Regis").focus();
		jQuery("#txtCEmail_Regis").val("");
		return false;
	}
	else
	{
		if(strEmail != strConfirmEmail)
		{
			jQuery("#txtErrorLoginEmail_Regis").html("&nbsp;");
			jQuery("#txtErrorLoginEmail2_Regis").html("không giống nhau");
			jQuery("#txtCEmail_Regis").focus();
			return false;
		}
		else
		{
			jQuery("#txtErrorLoginEmail2_Regis").html("&nbsp;");
			jQuery("#txtErrorLoginEmail_Regis").html("&nbsp;");
		}
	}

	if(strFirstName == '' || strFirstName=="Họ và Tên" || strFirstName==' Họ và Tên')
	{
		jQuery("#txtErrorFullname_Regis").html("Vui lòng nhập Họ và Tên");
		jQuery("#txtErrorFullname_Regis").focus();
		jQuery("#txtErrorFullname_Regis").val("");
		return false;
	}
	else if(intGenger == '')
	{
		jQuery("#txtErrorFullname_Regis").html("&nbsp;");
		jQuery("#txtErrorGender_Regis").html("Bạn vui lòng chọn giới tính!");
		return false;
	}
	else if(strDay=='' || strMonth=="" || strYear=='')
	{
		jQuery("#txtErrorGender_Regis").html("&nbsp;");
		jQuery("#txtErrorBirthday_Regis").html("Bạn vui lòng chọn ngày tháng năm sinh!");
		return false;
	}
	else if(strCity == '')
	{
		jQuery("#txtErrorBirthday_Regis").html("&nbsp;");
		jQuery("#txtErrorCity_Regis").html("Bạn vui lòng chọn tỉnh/thành phố!");
		return false;
	}
	else
	{
		jQuery("#txtErrorCity_Regis").html("&nbsp;");
	}
	if(strCaptcha == '' || strCaptcha == 'Nhập mã xác nhận' || strCaptcha == ' Nhập mã xác nhận')
	{
		jQuery("#txtErrorDKCode_Regis").html("Vui lòng nhập mã xác nhận");
		jQuery("#txtCaptcha_Regis").focus();
		jQuery("#txtCaptcha_Regis").val("");
		return false;
	}
	else
	{
		jQuery("#txtErrorFullname_Regis").html("&nbsp;");
		jQuery("#txtErrorGender_Regis").html("&nbsp;");
		jQuery("#txtErrorBirthday_Regis").html("&nbsp;");
		jQuery("#txtErrorCity_Regis").html("&nbsp;");
		jQuery("#txtErrorDKCode_Regis").html("&nbsp;");
	}

	if(!intCheckRight)
	{
		alert("Vui lòng check chọn Tôi cam kết đồng ý.");
		return false;
	}
	return true;
}
