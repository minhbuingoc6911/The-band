function checkInvalidUsers()
{
	strUsername 	= trim(jQuery("#txtUsername").val());
	strPassword 	= trim(jQuery("#txtPassword").val());
	strConfirmPass 	= trim(jQuery("#txtCPassword").val());
	strLastName 	= "";
	strFirstName 	= trim(jQuery("#txtFullname").val());
	strEmail 		= trim(jQuery("#txtEmail").val());
	strConfirmEmail = trim(jQuery("#txtCEmail").val());
	strCaptcha 		= trim(jQuery("#txtCaptcha").val());
	intCheckRight   = jQuery('input[name=chkRight]').is(':checked');
	strDay			= jQuery('#txtDay').val();
	strMonth 		= jQuery('#txtMonth').val();
	strYear			= jQuery('#txtYear').val();
	strCity			= jQuery('#txtCity').val();
	intGenger		= jQuery('#txtGender').val();
	strBirthDay    = strDay+'-'+strMonth+'-'+strYear;

	if(strUsername == '' ||  (strUsername == 'Nhập Username' || strUsername == ' Nhập Username'))
	{
		jQuery("#txtErrorUser").html("Vui lòng nhập Tài khoản!");
		jQuery("#txtUsername").focus();
		jQuery("#txtUsername").val("");
		return false;
	}
	else
	{
		if(strUsername.length < 6)
		{
			jQuery("#txtErrorUser").html("6 ký tự trở lên");
			jQuery("#txtUsername").focus();
			return false;
		}

		if(strUsername.indexOf('<') != -1 || strUsername.indexOf('>') != -1 || !CheckUsernameChar(strUsername))
		{
			jQuery("#txtErrorUser").html("không hợp lệ");
			jQuery("#txtUsername").focus();
			return false;
		}
		jQuery("#txtErrorUser").html("");
		checkUsers('boxlogin',strUsername);
	}

