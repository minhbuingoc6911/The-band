			  {
				  jQuery("#txtErrorDKCode").html("Captcha không đúng!");
				  if(strType == 'boxlogin')
					changeCaptchaLogin();
				  else if(strType == 'boxmap')
					changeCaptchaLogin_Map();
				  else if(strType == 'boxlogin_Regis')
					changeCaptchaLogin_Regis();
				  return false;
			  }
			  else if(data==1)
			  {
				  alert("Xin lỗi " + strUsername + " không thể dùng");
				  return false;
			  }
			  else if(data==2)
			  {
				  alert("Xin lỗi " + strEmail + " không thể dùng");
